#!/usr/bin/make -f
# See debhelper(7) (uncomment to enable)
# output every command that modifies files on the build system.
# export DH_VERBOSE = 1

DPKG_EXPORT_BUILDFLAGS = 1
include /usr/share/dpkg/default.mk

%:
	dh $@

override_dh_install:
	dh_install
	for x in if-up.d if-pre-up.d if-post-down.d; do \
	    mkdir -p debian/vxlan-huai-utils/etc/network/"$${x}"; \
	done
